{
  "name": "Assignment Tracker++",
  "version": "1.0.3",
  "manifest_version": 3,
  "description": "Changes new tabs page into an assignments tracker, editable directly & via right-clicking on assignments.",
  "author": "Natalie Stewart",
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": ["menu.js"],
    "persistent": true
  },
  "icons": {
     "128": "icon128.png",
     "32": "icon32.png"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ]
}
